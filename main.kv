        cols:1
        orientation: "tb-lr"
        spacing: dp(0)
        MDIconButton:
            id: menu_caller
            icon: "home-map-marker"
            font_size: 80
            theme_text_color: "Custom"
            text_color: "white"
            on_release: app.root.current = "Map View"





        MDGridLayout:
            cols:2
            size_hint_y: None
            height: self.minimum_height
            padding: dp(40), dp(0), dp(20), dp(20)
            spacing: dp(10)
            MDLabel:
                height: dp(35)
                text: "Welcome Home, Asher!"
                font_style: "H5"
                theme_text_color: "Custom"
                text_color: "white"
            MDIcon:
                icon: "home"
                font_size: dp(55)
                theme_text_color: "Custom"
                text_color: "white"
